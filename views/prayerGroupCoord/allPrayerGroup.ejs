<% include ./../partials/header %>

<div class="heading">
    <h1 class="text-center">Success</h1>
    <hr/>
    <p class="text-center">You have reported your prayer group participation successfully</p>
</div>

<main class="container">
    <div class="container">
        <div class="container form-group">
            <ol>
                <% foundReports.forEach((item)=>{ %>
                    <li><a href="/prayergroup/all/<%= item._id %>"><%= moment(item.date).format('YYYY MM DD') %></a></li>
                <% }) %> 
            </ol>
        </div>
    </div>
</main>
<% include ./../partials/footer %>