<% include ./../partials/header %>

<div class="heading">
    <h1 class="text-center">Prayer Group Report</h1>
    <hr/>
    <p class="text-center">Prayer Group Report</p>
    <a href="/prayergroup/admin" class="btn btn-sm">Back</a>
</div>

<main class="container" id="prayer-group-coord">
    <div class="container">
        <div class="container">
            <ol>
                <h4>LMA</h4>
                <% oneReport.attendance.forEach((item) => { %>
                    <% if(item.status == "LMA"){ %>
                        <li><%= item.firstname %> <%= item.surname %> | <%= item.arrivalTime %> | <%= item.zone %> </li>
                    <% } %>
                <% }) %> 
                <br>
                <h4>Cell Leaders</h4>
                <% oneReport.attendance.forEach((item) => { %>
                    <% if(item.status == "Cell Leader"){ %>
                        <li><%= item.firstname %> <%= item.surname %> | <%= item.arrivalTime %> | <%= item.zone %> </li>
                    <% } %>
                <% }) %>
                <br>
                <h4>Workers</h4>
                <% oneReport.attendance.forEach((item) => { %>
                    <% if(item.status == "Worker"){ %>
                        <li><%= item.firstname %> <%= item.surname %> | <%= item.arrivalTime %> | <%= item.zone %> </li>
                    <% } %>
                <% }) %> 
                <br>
                <h4>Members</h4>
                <% oneReport.attendance.forEach((item) => { %>
                    <% if(item.status == "Member"){ %>
                        <li><%= item.firstname %> <%= item.surname %> | <%= item.arrivalTime %> | <%= item.zone %> </li>
                    <% } %>
                <% }) %>  
            </ol>
        </div>
    </div>
</main>
<% include ./../partials/footer %>