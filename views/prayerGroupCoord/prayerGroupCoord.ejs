<% include ./../partials/header %>

<div class="heading">
    <h1 class="text-center">Welcome</h1>
    <hr/>
    <p class="text-center">Enter your credentials here!</p>
</div>

<main class="container">
    <div class="container">
        <form action="/prayergroup/<%= coordId %>" method="POST" class="form-group">
            <input class="form-control my-1 col-md-6" type="text" name="firstname" placeholder="Firstname"/>
            <input class="form-control my-1 col-md-6" type="text" name="surname" placeholder="Surname"/>
            <input class="form-control my-1 col-md-6" type="time" name="arrival"/>
            <input class="form-control my-1 col-md-6" type="text" name="zone" placeholder="Zone"/>
            <select class="form-control my-1 col-md-6" name="zone">
                <option value="--">-Select your Zone-</option>
                <option value="Akoka">Akoka</option>
                <option value="Bariga">Bariga</option>
                <option value="Ketu">Ketu</option>
                <option value="Mushin">Mushin</option>
                <option value="Sabo">Sabo</option>
                <option value="Somolu">Somolu</option>
                <option value="Surulere">Surulere</option>
            </select>
            <input class="form-control my-1 col-md-6" type="submit"/>
        </form>
    </div>
</main>
<% include ./../partials/footer %>